$('.tutorials').append("<%= j render 'static_pages/tutorials', tutorials: @tutorials %>");
<% if @tutorials.next_page %>
  $('.pagination').replaceWith('<%= will_paginate(@tutorials) %>');
  addthis.toolbox(".tutorial-box");
  $('.tutorial-text .add-this').hide();
  
<% else %>
  $('.pagination').remove();
<% end %>
<% sleep 2 %>