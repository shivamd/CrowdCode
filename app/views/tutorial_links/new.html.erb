<h1>Submit a Tutorial</h1>


<%= form_for @tut_link do |f| %>
  <%= render 'shared/error_messages', object: @tut_link %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :url %>
  <%= f.text_field :url %>

  <small>e.g "ruby ruby-on-rails javascript oauth"</small>
  <%= label_tag :tags %>
  <%= text_field_tag :tag %>

  <%= f.submit "Submit tutorial" %>
<% end %>

